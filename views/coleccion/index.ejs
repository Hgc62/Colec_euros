<% if (coleccionista) { %>
    <h1>MI COLECCIÓN DE EUROS</h1>
<% } else { %>
    <h1>CONSULTA EN COLECCIÓN EUROS</h1> 
<% } %>

<div class="botones_eleccion">
    <a href="/coleccion/formulario?nombre=<%= coleccionista.nombre %>">
        <button id="monedas_curso" class="boton_datos" > COLECCIÓN EUROS </button>    
    </a>
    <a href="/coleccion/formulario?tipo=conmemorativa&nombre=<%= coleccionista.nombre %>">
        <button id="conmemorativas" class="boton_datos" > CONMEMORATIVAS</button>    
    </a>
</div>
<% if (locals.loginUser.displayName !== "Invitado") { %>
    <a href="/coleccion/new" class="button">Añadir nueva moneda</a>
    <a href="/coleccion/new?tipo=conmemorativa" class="button">Añadir nueva conmemorativa</a>
<% } %>